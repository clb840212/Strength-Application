<%= simple_form_for @workout do |f| %>
<%= f.input :date, label: "Date" %>
<%= f.input :workout, label: "Body area of emphasis", input_html: { class: "form-control"} %>
<%= f.input :length, label: "Workout duration (minutes)", input_html: { class: "form-control"} %>
<%= f.input :mood, label: "How are you feeling?", input_html: { class: "form-control"} %>
<%= f.button :submit, label: "Update" %>
<% end %>

<%= link_to 'Cancel', root_path %>
